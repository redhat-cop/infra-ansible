# This file is autogenerated by Ansible {{ ansible_version.full }} on {{ ansible_date_time.date }}

DEFAULT vesamenu.c32
PROMPT 0
MENU TITLE {{ pxe_menu_title | default(default_pxe_menu_title) }}
TIMEOUT {{ pxe_timeout | default(default_pxe_timeout) }}
TOTALTIMEOUT 6000
ONTIMEOUT local

LABEL local
        MENU LABEL (local)
        MENU DEFAULT
        LOCALBOOT -1

{% for entry in pxe_entries %}
LABEL {{ entry.name }}
        MENU LABEL {{ entry.menu_label }}
        kernel {{ entry.kernel }}
        append {{ entry.append }} ip={{ ip_config | default('dhcp') }}
        ipappend 2

{% endfor %}
