VLAN="yes"
TYPE="Vlan"
DEVICE="{{ ifcfg.device }}"
NAME="{{ name | default(ifcfg.device) }}"
PHYSDEV="{{ ifcfg.physdev }}"
VLAN_ID="{{ ifcfg.vlan_id }}"
BOOTPROTO="{{ ifcfg.bootproto | default('none') }}"
{% if ifcfg.ipaddr is defined %}
IPADDR="{{ ifcfg.ipaddr }}"
{% endif %}
{% if ifcfg.netmask is defined %}
NETMASK="{{ ifcfg.netmask }}"
{% endif %}
{% if ifcfg.gateway is defined %}
GATEWAY="{{ ifcfg.gateway }}"
{% endif %}
{% if ifcfg.dns1 is defined %}
DNS1="{{ ifcfg.dns1 }}"
{% endif %}
{% if ifcfg.dns2 is defined %}
DNS2="{{ ifcfg.dns2 }}"
{% endif %}
DEFROUTE="yes"
IPV4_FAILURE_FATAL="no"
ONBOOT="{{ ifcfg.onboot | default('yes') }}"

